{
  "name": "nestjs",
  "version": "0.0.1",
  "private": true,
  "packageManager": "bun@1.0.0+",
  "description": "",
  "author": "",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.0.0"
  },
  "engineStrict": true,
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "orm": "dotenv -- mikro-orm",
    "migration:initial": "bunx mikro-orm migration:create --initial",
    "migration:create": "bunx mikro-orm migration:create",
    "migration:up": "bunx mikro-orm migration:up",
    "migration:down": "bunx mikro-orm migration:down",
    "migration:pending": "bunx mikro-orm migration:pending",
    "migration:list": "bunx mikro-orm migration:list",
    "yac": "npx httpyac ./api-test.http",
    "prepare": "husky",
    "cz": "cz",
    "check-versions": "node scripts/check-versions.js"
  },
  "dependencies": {
    "@fastify/cookie": "11.0.2",
    "@mikro-orm/core": "6.3.0",
    "@mikro-orm/nestjs": "6.1.1",
    "@mikro-orm/postgresql": "6.3.0",
    "@nestjs/axios": "4.0.1",
    "@nestjs/common": "11.0.5",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.0.5",
    "@nestjs/platform-fastify": "11.1.6",
    "@nestjs/schedule": "6.0.1",
    "@nestjs/terminus": "11.0.0",
    "@nestjs/throttler": "6.4.0",
    "@sapphire/snowflake": "3.5.5",
    "argon2": "0.44.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "dotenv-cli": "11.0.0",
    "es-toolkit": "1.42.0",
    "fastify": "5.6.2",
    "ioredis": "5.8.2",
    "nestjs-pino": "4.4.1",
    "pino": "9.14.0",
    "pino-http": "10.5.0",
    "pino-pretty": "13.1.2",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "statuses": "2.0.2",
    "systeminformation": "5.27.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "6.0.0",
    "@mikro-orm/migrations": "6.3.0",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@mikro-orm/cli": "6.3.0",
    "@nestjs/cli": "11.0.5",
    "@nestjs/schematics": "11.0.5",
    "@nestjs/testing": "11.0.5",
    "@swc/cli": "0.6.0",
    "@swc/core": "1.15.3",
    "@types/jest": "30.0.0",
    "@types/node": "22.10.7",
    "@types/statuses": "2.0.6",
    "@types/supertest": "6.0.2",
    "commitizen": "4.3.1",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "17.2.3",
    "eslint": "9.38.0",
    "globals": "16.0.0",
    "httpyac": "6.16.7",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "supertest": "7.1.4",
    "ts-jest": "29.4.5",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}